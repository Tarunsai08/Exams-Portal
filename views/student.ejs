<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Student</title>
    <meta name="author" content="Tarun Sai" />
    <meta name="description" content="" />

    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/tailwindcss/2.2.19/tailwind.min.css"
      rel="stylesheet"
    />
    <style>
      @import url("https://fonts.googleapis.com/css?family=Karla:400,700&display=swap");
      .font-family-karla {
        font-family: karla;
      }
      .bg-sidebar {
        background: #4caf50; /* Forest green */
      }
      .cta-btn {
        color: #43a047; /* Jade green */
      }
      .upgrade-btn {
        background: #00c853; /* Teal green */
      }
      .upgrade-btn:hover {
        background: #007b3f; /* Darker teal green */
      }
      .active-nav-link {
        background: #1b9f73; /* Sea green */
      }
      .nav-item:hover {
        background: #1b9f73; /* Sea green */
      }
      .account-link:hover {
        background: #388e3c; /* Dark olive green */
      }
    </style>
  </head>
  <body class="bg-gray-200 font-family-karla flex">
    <aside class="relative bg-sidebar h-screen w-64 hidden sm:block shadow-xl">
      <div class="p-6">
        <a
          href="/student"
          class="text-white text-3xl font-semibold uppercase hover:text-gray-300"
          >Student</a
        >
      </div>
      <nav class="text-white text-base font-semibold pt-3">
        <a
          href="/student"
          class="flex items-center active-nav-link text-white py-4 pl-6 nav-item"
        >
          <i class="fas fa-tachometer-alt mr-3"></i>
          Dashboard
        </a>
      </nav>
    </aside>

    <div class="w-full flex flex-col h-screen overflow-y-hidden">
      <!-- Desktop Header -->
      <header class="w-full items-center bg-white py-2 px-6 hidden sm:flex">
        <div class="w-1/2">
          <h1 class="text-xl font-bold text-gray-700">Exam Portal</h1>
        </div>
        <div x-data="{ isOpen: false }" class="relative w-1/2 flex justify-end">
          <button
            @click="isOpen = !isOpen"
            class="realtive z-10 w-12 h-12 rounded-full overflow-hidden border-4 border-green-400 hover:border-green-300 focus:border-gray-300 focus:outline-none"
          >
            <img
              src="data:image/jpeg;base64,"
            />
          </button>
          <button
            x-show="isOpen"
            @click="isOpen = false"
            class="h-full w-full fixed inset-0 cursor-default"
          ></button>
          <div
            x-show="isOpen"
            class="absolute w-32 bg-white rounded-lg shadow-lg py-2 mt-16"
          >
            <a
              href="/student"
              class="block px-4 py-2 account-link hover:text-white"
              >Home</a
            >
            <a
              href="/logout"
              class="block px-4 py-2 account-link hover:text-white"
              >Sign Out</a
            >
          </div>
        </div>
      </header>

      <div id="content" class="m-4 overflow-x-hidden border-t flex flex-col">
        <h2
          class="text-sky text-3xl font-semibold uppercase hover:text-sky-300"
        >
          Your Marks
        </h2>
        <div class="flex flex-wrap my-4">
          <% for (const [subject, value] of Object.entries(student.marks)) { %>
          <div class="w-full md:w-1/2 px-4 mb-4">
            <div class="bg-white rounded-lg shadow-md p-4">
              <h3 class="text-lg font-semibold text-gray-700 mb-2">
                <%= subject %>
              </h3>
              <p class="text-gray-600"><%= value %></p>
            </div>
          </div>
          <% } %>
        </div>
      </div>
    </div>

    <!-- AlpineJS -->
    <script
      src="https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.x.x/dist/alpine.min.js"
      defer
    ></script>
    <!-- Font Awesome -->
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/js/all.min.js"
      integrity="sha256-KzZiKy0DWYsnwMF+X1DvQngQ2/FxF7MF3Ff72XcpuPs="
      crossorigin="anonymous"
    ></script>
  </body>
</html>
